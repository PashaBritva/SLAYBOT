{
  "name": "discord-js-bot",
  "version": "1.5.1",
  "description": "A multipurpose Discord bot built using modern Node.js and discord.js",
  "main": "bot.js",
  "author": "isaac",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon .",
    "start": "node bot.js"
  },
  "homepage": "https://github.com/PashaBritva/SLAYBOT#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PashaBritva/SLAYBOT.git"
  },
  "engines": { "node": ">=18.0.0 <19.0.0" },
  "dependencies": {
    "discord.js": "13.15.1",
    "erela.js": "2.3.3",
    "erela.js-deezer": "1.0.7",
    "erela.js-spotify": "1.2.0",
    "node-fetch": "3.3.2",
    "simple-node-logger": "21.8.12",
    "@vitalets/google-translate-api": "9.2.1",
    "iso-639-1": "2.1.12",
    "axios": "1.6.8",
    "chalk": "4.1.2",
    "mongoose": "8.16.1",
    "dotenv": "16.3.2",
    "express": "4.19.2",
    "express-session": "1.17.3",
    "moment": "2.29.4",
    "ejs": "3.1.9",
    "nekos.life": "2.0.9",
    "outdent": "0.8.0",
    "pretty-ms": "7.0.1",
    "string-progressbar": "1.0.4",
    "table": "6.8.0",
    "timestamp-to-date": "1.1.0",
    "twemoji-parser": "13.1.0",
    "btoa": "1.2.1",
    "country-language": "0.1.7",
    "fixedsize-map": "1.0.1",
    "sourcebin_js": "0.0.3-ignore",
    "ascii-table": "0.0.9",
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "nodemon": "3.0.3",
    "prettier": "3.2.5"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "multipurpose"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@commands": "src/commands/",
    "@features": "src/features/",
    "@schemas": "src/schemas/",
    "@src": "src/",
    "@utils": "src/utils/"
  },
  "bugs": {
    "url": "https://github.com/PashaBritva/SLAYBOT/issues"
  },
  "directories": {
    "doc": "docs"
  }
}
