{
  "name": "slaybot",
  "version": "2.0.0",
  "description": "A multipurpose Discord bot built using modern Node.js and discord.js",
  "main": "bot.js",
  "author": "isaac",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon .",
    "start": "node bot.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "homepage": "https://github.com/PashaBritva/SLAYBOT#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PashaBritva/SLAYBOT.git"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "9.2.1",
    "ascii-table": "0.0.9",
    "axios": "^1.11.0",
    "chalk": "^4.1.2",
    "country-language": "0.1.7",
    "discord-giveaways": "6.0.1",
    "discord.js": "^14.22.1",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "erela.js": "2.3.3",
    "erela.js-deezer": "1.0.7",
    "erela.js-spotify": "1.2.0",
    "express": "4.21.2",
    "express-session": "1.18.2",
    "fixedsize-map": "1.0.1",
    "iso-639-1": "3.1.0",
    "module-alias": "2.2.2",
    "moment": "2.30.1",
    "mongoose": "^8.18.1",
    "nekos.life": "2.0.9",
    "outdent": "0.8.0",
    "pretty-ms": "9.0.0",
    "simple-node-logger": "21.8.12",
    "sourcebin_js": "0.0.3-ignore",
    "string-progressbar": "1.0.4",
    "table": "6.8.2",
    "timestamp-to-date": "1.1.0",
    "twemoji-parser": "^14.0.0"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "eslint-plugin-jsdoc": "48.2.0",
    "nodemon": "3.1.7",
    "prettier": "3.3.3"
  },
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "multipurpose"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@commands": "src/commands/",
    "@features": "src/features/",
    "@schemas": "src/schemas/",
    "@src": "src/",
    "@utils": "src/utils/"
  },
  "bugs": {
    "url": "https://github.com/PashaBritva/SLAYBOT/issues"
  },
  "directories": {
    "doc": "docs"
